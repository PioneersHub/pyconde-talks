# Let Compose delegate the build to Bake
COMPOSE_BAKE=true

# Database
POSTGRES_USER=XXXXXXXXXXXX
POSTGRES_PASSWORD=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
POSTGRES_DB=XXXXXXXXXXXX

# General
DJANGO_SETTINGS_MODULE=pyconde_talks.settings

# Static and media files
DJANGO_STATIC_ROOT=/staticfiles
DJANGO_MEDIA_ROOT=/media/
MOUNT_MEDIA_ROOT=/var/opt/talks.pycon.de/media/
